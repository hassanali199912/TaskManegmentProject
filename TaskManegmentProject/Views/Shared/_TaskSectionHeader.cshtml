@using TaskManegmentProject.DBcontcion
@model WorkSpace
@{
	ViewData["workSpaceId"] = Model.Id;
	List<WorkSpace> myWorkSpacesList = (List<WorkSpace>)ViewData["workSpaceList"];
	ViewData["SelectedWorkSpace"] = Model;
}
<div class="task-section-header card-header">

	<partial name="_SelectedListWorkSpace" model="@myWorkSpacesList"  />


	<div class="header-controls">
		
		<partial 
			name="_MembersDataWorkSpace"
			model="@Model.Members" />
		

		<div class="notification-bell">
			<button onclick="ToggleBTN('notification-bell-popup')"><i class="fa-regular fa-bell"></i></button>
			<div class="notification-bell-popup"
				 id="notification-bell-popup"
			@* true => active *@
				>
				<p>Notifications</p>
				<hr>
				<div class="notifications-box">
					<div class="notifications-box-item">
						<img src="data:image/jpeg;base64,"
							 alt="User image">
						<div class="notifications-box-item-info">
							<h5>hassan ali</h5>
							<p>add new task</p>
						</div><span> 35min</span>
					</div>
					<div class="notifications-box-item">
						<img src="data:image/png;base64,"
							 alt="User image">
						<div class="notifications-box-item-info">
							<h5>GG</h5>
							<p>complete taske</p>
						</div><span> 35min</span>
					</div>
					<div class="notifications-box-item">
						<img src="data:image/png;base64,"
							 alt="User image">
						<div class="notifications-box-item-info">
							<h5>hassan ali</h5>
							<p>delete task</p>
						</div><span> 35min</span>
					</div>
				</div>
			</div>
		</div>
		
		<button ><i class="fa fa-plus"></i></button>
		
		<div class="mode-toggle-home">
			<button class="mode-btn" id="btn-mode" onclick="swichMode()">
				<img src="~/img/icons/moon.svg" id="mode-icon" alt="Mode Svg" />
			</button>
		</div>

	</div>
</div>




<partial name="_FlipModeJs" />
<partial name="_ValidationScriptsPartial"/>

<script>

		function ToggleBTN(ids){
			$(`#${ids}`).toggleClass("active");
		}

</script>